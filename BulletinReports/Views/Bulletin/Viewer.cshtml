@model BulletinReports.Models.BulletinsViewModel.BulletinViewModel
@using BulletinReport.Common;
@using BulletinReport.Common.App_LocalResources;

@{
    ViewBag.Title = "Viewer";
}

<section class="main_wrapper">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div class="login">
                    <div class="form-row">
                        <div class="section-title">
                            <h3>@Resource.BulletinReportAttachment</h3>
                        </div>
                    </div>
                    <div class="login_form">
                        <div class="container">
                            <iframe id="DocViewer" src="@ViewBag.FilePath" style="width: 100%; height: 929px;"></iframe>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
